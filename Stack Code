#include <bits/stdc++.h>
using namespace std;

#define MAX 10
//int size = 0;

struct Stack{
    int items[MAX];
    int top;
};

void createEmptyStack(Stack *s){
    s->top = -1;
}

int IsFull(Stack *s){
    if(s->top==MAX-1)
        return 1;
    else
        return 0;
}

int IsEmpty(Stack *s){
    if(s->top==-1)
        return 1;
    else
        return 0;
}
void push(Stack *s, int newitem){
    if(IsFull(s))
        cout << "Stack Full\n";
    else{
        s->top++;
        s->items[s->top] = newitem;
    }
    //size++;
}

void pop(Stack *s){
    if(IsEmpty(s))
        cout << "Empty Stack\n";
    else{
        cout << "Item Popped " << s->items[s->top];
        s->top--;
    }
    //size--;
}
void printStack(Stack *s){
    for(int i=0; i<=s->top; i++)
        cout << s->items[i] << " ";
    cout << endl;
}

int main() {
    int n, x;
    cin >> n;
    Stack *s = new Stack;
    //Stack *s = (Stack *)malloc(sizeof(Stack));
    
    createEmptyStack(s);
    
    for(int i=0; i<n; i++){
        cin >> x;
        push(s, x);
    }
    //push(s, x);
    
    cout <<"Stack: ";
    printStack(s);
    
    pop(s);
    
    cout << "\nAfter Popping\n";
    cout <<"Stack: ";
    printStack(s);
    
    cout << endl;

    return 0;
}
